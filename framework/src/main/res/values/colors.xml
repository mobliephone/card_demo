<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--状态栏背景色-->
    <color name="colorPrimaryDark">@color/primary_dark</color>
    <!--App bar 上的标题与更多菜单中的文字颜色-->
    <color name="textColorPrimary">@color/white</color>
    <!--Actionbar 的背景色设定在 style 中的 colorPrimary。-->
    <color name="colorPrimary">@color/primary_dark</color>
    <!--各控制元件(如：check box、switch 或是 radoi) 被勾选 (checked) 或是选定 (selected) 的颜色。-->
    <color name="colorAccent">@color/primary_dark</color>
    <!--各控制元件的预设颜色。-->
    <color name="colorControlNormal">@color/white</color>
    <!--导航栏的背景色，但只能用在 API Level 21 (Android 5) 以上的版本-->
    <color name="navigationBarColor">@color/primary_dark</color>
    <color name="primary_dark">#607de3</color>
    <color name="white">#ffffff</color>
    <color name="gray">#eeeeee</color>
    <color name="color_red">#f65b66</color>
    <!--弹出框-->
    <color name="dialogui_btn_normal">#ffffff</color>
    <color name="dialogui_btn_pressed">#eeeeee</color>
    <color name="datepick_surance_blue">#3598da</color>


    <color name="textGray">#696969</color>
</resources>
